<Window x:Class="ChatOnline.PrivatChat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatOnline"
        mc:Ignorable="d"
        Title="PrivatChat" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="0.2*"></RowDefinition>
        </Grid.RowDefinitions>
        <ListBox ItemsSource="{Binding ListMmessage}"></ListBox>
        <TextBox x:Name="textboxMsg" Grid.Row="1" KeyDown="TextBox_KeyDown"></TextBox>
    </Grid>
</Window>
